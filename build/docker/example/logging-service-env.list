#################
# LogDB
#################
LOG_DB_CONNECTION_STR=<mongodb-atlas-or-other-server-e.g.xxxx.mongodb.net/test?retryWrites=true&w=majority>
LOG_DB_CONNECTION_PREFIX=<emtpy or +srv if atlas>
# should be secret:
LOG_DB_USERNAME=<db-username>
LOG_DB_PASSWORD=<db-password>

#################
# general db client settings
#################
DB_TIMEOUT=30
DB_IDLE_CONN_TIMEOUT=45
DB_MAX_POOL_SIZE=8
DB_DB_NAME_PREFIX=<db name prefix>

#################
# grpc services
#################
LOGGING_SERVICE_LISTEN_PORT=5006